        window.location.href = "#listIPcomehere";

        function reportIPcomehere()
        {
            window.location.href = "/Admin/ReportListIPComeHere";
        }

         function deleteIPcomehere()
        {
            var con = confirm('Are you sure?');
            if (con == true)
            window.location.href = "/Admin/DeleteIPComeHere";
        }

        function listIPcomehere()
        {
            window.location.href = "#listIPcomehere";
        }

        function xong()
        {
             document.getElementById("ok").click();
        }

        function chonall()
        {
            var n = new Number(document.getElementById("count").value) - 1;
            for (var i = 0; i < n;i++)
            {
                document.getElementById("check-"+i).checked = true;
            }
            document.getElementById("ok").click();
        }

        function boall()
        {
            var n = new Number(document.getElementById("count").value) - 1;
            for (var i = 0; i < n;i++)
            {
                document.getElementById("check-"+i).checked = false;
            }
            document.getElementById("ok").click();
        }
     </script>


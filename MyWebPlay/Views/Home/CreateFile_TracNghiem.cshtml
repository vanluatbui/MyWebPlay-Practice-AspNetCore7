        for (var i = 0; i< s_num.length; i++)
        {
            var num_s = s_num[i].split("-");
            arr.push(num_s[0]);
            arr.push(num_s[1]);
        }

        var min = Math.min.apply(null,arr);
        var max = Math.max.apply(null,arr);

        var checkX = [];

         for (var i = min ; i <= max ;i++)
         checkX.push(0);
        
        for (var i = 0; i< s_num.length; i++)
        {
            var num_s = s_num[i].split("-");
           for (var j = num_s[0]; j<= num_s[1]; j++)
           checkX[j] = 1;
        }

         for (var i = min ; i <= max ;i++)
         {
             if (checkX[i] == 1)
